{
  "version": "1.0.0",
  "name": "mongo",
  "container": {
    "image": "mongo",
    "tag": "latest",
    "command": "",
    "arguments": []
  },
  "resources": {
    "cpu": 1,
    "memory": 512
  },
  "ports": [
    {
      "container": 27017,
      "external": 27017
    }
  ],
  "env": [
    {
      "name": "MONGO_INITDB_ROOT_USERNAME",
      "value": "root"
    },
    {
      "name": "MONGO_INITDB_ROOT_PASSWORD",
      "value": "example"
    }
  ]
}
